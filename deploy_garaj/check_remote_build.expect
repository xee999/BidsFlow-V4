#!/usr/bin/expect -f
set timeout 30
set ip [lindex $argv 0]
set port [lindex $argv 1]
set password [lindex $argv 2]

spawn ssh -o StrictHostKeyChecking=no -p $port root@$ip
expect {
  "password:" { send "$password\r" }
}
expect -re "#"

send "ls -lh /var/www/bidsflow/dist/index.html\r"
expect -re "#"

send "ls -lh /var/www/bidsflow/dist/assets/index-*.js\r"
expect -re "#"

send "exit\r"
expect eof
